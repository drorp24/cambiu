<div id="exchange_voucher" class="page" style="display: none">

    <div class="row exchange_voucher_bg">
        <div class="col-md-12 col-xs-12">
            <%= image_tag 'voucher_bg.png' %>
        </div>
    </div>

  <div class="row">
    <div class="col-md-11 col-xs-11">
      <div class="voucher_heading clearfix">
        <div class="col-xs-10 col-md-10">
          <div class="voucher_title">
            <span>Voucher</span>
            <span class="voucher_number" data-field="e_voucher_number">28395</span>
          </div>
          <div class="voucher_validity">
            <span><%= fa_icon "clock-o", text: "  Valid until", left: true %></span>
            <span class="voucher_expiry" data-field="e_voucher_expiry">5:45 pm</span>
          </div>
        </div>
        <div class="col-xs-2 col-md-2">
          <div class="printer_icon">
            <%= fa_icon "print" %>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="exchange_voucher_banner">
    <div class="row">
       <div class="col-md-12 col-xs-12">
        <div class="col-md-12 col-xs-12">
          <div class="es_name" data-field="e_name">Moneycorp Currency Exchange Service</div>
          <div class="es_address" data-field="e_address">4 Sloan Square bla bla bla</div>
          <div class="es_opening_hours" data-field="e_opening_hours">Open Today: 10:00am  -  5:00pm</div>
        </div>
      </div>
    </div>
  </div>

  <div class="exchange_voucher_directions">
    <div class="row">
      <div class="col-md-12 col-xs-12">
        <div class="col-md-12 col-xs-12">
          <div class="directions"><%= fa_icon "map-marker", text: "  Get directions", left: true %></div>
        </div>
      </div>
    </div>
  </div>


</div>