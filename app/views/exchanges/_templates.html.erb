<!--   Exchange row template  -->
<div class="exchange_row template" style="display: none;">
  <div class="list-group-item directions"

    data-href                                   = ''
    data-href-page                              = 'exchanges'
    data-href-id                                = ''
    data-href-pane                              = 'voucher'

    data-model                                  = 'exchange'
    data-service-type                           = ''
    data-delivery-tracking                      = 'null'
    data-delivery-redirect-if-selected          = 'null'
    data-exchange-selection                     = 'true'
    data-exchangeid                             = ''
    data-voucher                                = 'true'
    data-exchange-name                          = ''
    data-lat                                    = ''
    data-lng                                    = ''
  >

    <div class="row">
      <%= render 'exchanges/list/exchange_sum_template' %>
    </div>
  </div>
</div>

<!--   Exchange InfoWindow template  -->
<div class="exchange_window template">
  <div data-id="" class="exchange_window_sum">
    <div class="exchange_window_name"></div>
    <div class="exchange_window_quote"></div>
    <div class="exchange_window_distance"></div>
  </div>
  <div data-id="" class="exchange_window_det">
    <div class="exchange_window_name"></div>
    <div class="exchange_window_address"></div>
    <div class="exchange_window_quote"></div>
    <div class="exchange_window_open"></div>
  </div>
</div>