<div class="container-fluid">
  <a name="exchanges"></a>
  <div id="exchanges_page">
    <%= render 'exchanges/params/index' %>
    <div class="row" id="exchanges_content">
      <div id="exchanges" class="col-md-4 col-xs-12">
        <%= render 'exchanges/list/index' %>
        <%= render 'exchanges/summary/index' %>

        <div class="flip-container">
          <div class="flipper">
            <div class="front">
              <%= render 'exchanges/deal/index' %>
            </div>
            <div class="back">
              <%= render 'exchanges/voucher/index' %>
            </div>
          </div>
        </div>

       </div>
      <div id="map_container" class="col-md-8">
        <div id="directions-panel" class="panel" style="display:none"></div>
        <div id="map-canvas" data-clicked="homepage map"></div>
      </div>
    </div>
  </div>


</div>

<%= render 'exchanges/templates' %>