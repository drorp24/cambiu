		<div class="main clearfix">
			<div class="marketing col-xs-12 col-md-5 col-md-offset-1">
				<div class="pre">
					<p></p>
				</div>
				<div class="heading">
					<p>Best way to<br/>exchange currency</p>
				</div>
				<div class="bullets">
					<ul class="list-unstyled">
                      <li>Best rates</li>
                      <li>Find offers nearest to your location</li>
                      <li>Over 10,000 branches worldwide</li>
                    </ul>
				</div>
			</div>
			<div class="moneybox col-xs-12 col-md-3 col-md-offset-1">
				<%= render 'search_form' %>
			</div>
		</div>
	</div>
